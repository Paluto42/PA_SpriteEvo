<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- 本组只使用Spine3.8 /4.1 版本 其他版本的Spine资源暂时不支持-->
    <!-- DefName格式: 组名/模组名 + Spine + ID--> 
	<!-- skelFormat: 骨架数据的格式,选择二进制数据或者JSON格式-->
	<!-- offset三维变量 指定场景内动画的偏移量 ,uioffset指定UI贴图的偏移量,Z轴调整投影距离-->
    <!-- assetBundle/folderPath: Asset_Spine/路径内的ab包文件全称 或者json文件夹名称-->
    <!-- atlas: ab包里指定atlas数据的文件全称(带拓展名)-->
    <!-- skeleton: ab包里指定skeleton数据的文件全称(带拓展名)-->
    <!-- shader: 保持默认为Spine/Skeleton -->
    <!-- materials/textures: 指定使用现成材质或PNG贴图,建议优先用material-->
    <!-- useStraightAlpha: 启用shader功能 不使用PMA混合模式贴图. 可解决渲染出现白边的问题 材质已经启用也要填true-->

    <PA_SpriteEvo.SpinePackDef>
		<defName>AK_Spine_Dusk_dynillust_E2</defName>
        <assetBundle>Spine/asset_dyn_suifamily</assetBundle>
		<offset>(0,0,0)</offset>
		<uioffset>(0,12,-18)</uioffset>
		<scale>(15,15)</scale>
		<!--rotation>(0,0,0)</rotation-->
		<props>
		    <!--version>3.8</version-->
			<atlas>dyn_illust_char_2015_dusk.atlas.txt</atlas>
			<skeleton>dyn_illust_char_2015_dusk.skel.bytes</skeleton>
			<!--shader>Spine-Skeleton.shader</shader-->
			<materials>
				<li>dyn_illust_char_2015_dusk_Material</li>
				<li>dyn_illust_char_2015_dusk_Material-Additive</li>
			</materials>
			<StraightAlphaInput>true</StraightAlphaInput>
		</props>
	</PA_SpriteEvo.SpinePackDef>

